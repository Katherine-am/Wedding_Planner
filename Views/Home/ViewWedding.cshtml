@model WrapperModel

<div class="container">
    <div class="row">
        <div class="col">
            <h3>@Model.newWedding.WedderOne & @Model.newWedding.WedderTwo</h3>
        </div>
        <div class="col">
            <a href="/Dashboard" class="btn btn-success">Dashboard</a>
            <a href="/SignOut" class="btn btn-dark">Log Out</a>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <p>@Model.newWedding.Date</p>
            <p>Guests</p>
            @{
                foreach(var guest in Model.newWedding.WeddingGuest)
                {
                    <p>@guest.User.FirstName @guest.User.LastName</p>
                }
            }
        </div>
        <div class="col">
            <div id="map"></div>
            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCrYEpsKhOJ9vtrNVnobGxE8S-ecaws8kw&callback=initMap" async defer>
            </script>
            <script>
                var map;
                var geocoder;
                function initMap(){
                    map = new google.maps.Map(document.getElementById('map'),{
                        zoom:8,
                        center:{lat:47.36,lng:-122.20},
                        height: 100%
                    });
                }
            </script>
        </div>
    </div>
</div>



